Algoritmo ejercicioOperadoresAritmeticos
	
	Imprimir 'Porporciona un valor pes: '
	Leer valor1
	
	Imprimir 'Porporciona otro valor pes: '
	Leer valor2
	
	Imprimir 'Resultado suma: ', valor1 + valor2;
	Imprimir 'Resultado resta: ', valor1 - valor2;
	Imprimir 'Resultado division: ', valor1 / valor2;
	Imprimir 'Resultado multiplicacion: ', valor1 * valor2;
	Imprimir 'Resultado resto division: ', valor1 % valor2;
	Imprimir 'Resultado potencia xd: ', valor1 ^ valor2;
	
FinAlgoritmo
